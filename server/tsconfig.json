{
    "compilerOptions": {
      "target": "ES2020",
      "module": "commonjs",
      "declaration": true,
      "sourceMap": true,
      "outDir": "dist",
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "esModuleInterop": true,
      "resolveJsonModule": true,
    },
    "include": [
      "src/**/*",
      "src/**/*.json"
    ]
  }
  